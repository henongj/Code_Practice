#include <string>
#include <vector>
#include <algorithm>
using namespace std;

vector<int> solution(vector<int> array, vector<vector<int>> commands) {
    vector<int> answer;
    
    for(int i = 0 ; i < commands.size() ; i++)
    {
        vector<int> t;
        int start = commands[i][0] - 1;
        int end = commands[i][1];
        for(int j = start ; j < end ; j++)
            t.push_back(array[j]);
        sort(t.begin(),t.end());
        answer.push_back(t[commands[i][2] - 1]);
    }
    return answer;
}
