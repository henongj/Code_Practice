#include <cstdio>
#include <iostream>
using namespace std;

inline void print_star(int n)
{
	for(int i=  0 ; i< n ; i++)
		printf("*");
}
int main(void)
{
	int n;
	cin >> n;
	
	for(int i = 1 ; i <= n ; i++)
	{
		print_star(i);
		puts("");
	}
	for(int i = n-1 ; i > 0 ; i--)
	{
		print_star(i);
		puts("");
	}
	return 0;
}
